FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.00448;

boundaryField
{
    bottomWall
    {
        type            nutUWallFunction;
        value           nonuniform 0();
    }
    topWall
    {
        type            nutUWallFunction;
        value           nonuniform 0();
    }
    sides1_half0
    {
        type            cyclic;
    }
    sides1_half1
    {
        type            cyclic;
    }
    sides2_half0
    {
        type            cyclic;
    }
    sides2_half1
    {
        type            cyclic;
    }
    inout1_half0
    {
        type            cyclic;
    }
    inout1_half1
    {
        type            cyclic;
    }
    inout2_half0
    {
        type            cyclic;
    }
    inout2_half1
    {
        type            cyclic;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.00448;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 0.00448;
    }
}


